{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 1,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureMySqlSource",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureDataLakeStoreWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"quoteAllText": true
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "sr_no",
									"type": "Int32"
								},
								"sink": {
									"path": "$['sr_no']"
								}
							},
							{
								"source": {
									"name": "employee_first_name",
									"type": "String"
								},
								"sink": {
									"path": "$['employee_first_name']"
								}
							},
							{
								"source": {
									"name": "employee_last_name",
									"type": "String"
								},
								"sink": {
									"path": "$['employee_last_name']"
								}
							},
							{
								"source": {
									"name": "age",
									"type": "Int32"
								},
								"sink": {
									"path": "$['age']"
								}
							},
							{
								"source": {
									"name": "school",
									"type": "String"
								},
								"sink": {
									"path": "$['school']"
								}
							},
							{
								"source": {
									"name": "college",
									"type": "String"
								},
								"sink": {
									"path": "$['college']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "AzureMySqlTable1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Json1",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "dataflow2",
				"type": "ExecuteDataFlow",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "dataflow2",
						"type": "DataFlowReference"
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					}
				}
			}
		],
		"annotations": []
	}
}